{
    "name": "solve-it-once/frost",
    "description": "Composer-based atomic template for Drupal",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Brad Czerniak",
            "role": "maintainer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "chosen"
                },
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "simple-icons/simple-icons",
                "version": "4.2.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "simple-icons"
                },
                "dist": {
                    "url": "https://github.com/simple-icons/simple-icons/archive/4.2.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.12",
        "cweagans/composer-patches": "~1.0",
        "drupal/address": "~1.0",
        "drupal/admin_toolbar": "^3.1",
        "drupal/advagg": "^4.0",
        "drupal/antibot": "^1.2",
        "drupal/auditfiles": "^3.0@beta",
        "drupal/better_exposed_filters": "~5.0",
        "drupal/bg_image_formatter": "^1.1",
        "drupal/block_class": "^1.0@alpha",
        "drupal/block_field": "^1.0@alpha",
        "drupal/captcha": "^1.0@beta",
        "drupal/chosen": "^3.0",
        "drupal/commerce": "^2",
        "drupal/commerce_add_to_cart_link": "^2.0",
        "drupal/commerce_paypal": "^1.0@RC",
        "drupal/commerce_recurring": "1.x-dev",
        "drupal/commerce_shipping": "^2.0@RC",
        "drupal/config_filter": "^2.2",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/config_split": "^2.0@beta",
        "drupal/core": "^9.2",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/ctools": "^3.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/diff": "^1.0@RC",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity": "~1.0",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_reference_display": "1.x-dev",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/facets": "^2.0",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "3.x-dev",
        "drupal/form_mode_manager": "^2.0@dev",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.0",
        "drupal/human_decimal": "^3.0",
        "drupal/iframe": "^2.6",
        "drupal/imageapi_optimize": "^4.0@beta",
        "drupal/imageapi_optimize_binaries": "^1.0@alpha",
        "drupal/imageapi_optimize_resmushit": "^2.0@beta",
        "drupal/imagemagick": "^3.3",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/jquery_ui_tabs": "^1.1",
        "drupal/jquery_ui_tooltip": "^1.1",
        "drupal/linked_field": "^1.1",
        "drupal/linkit": "5.x-dev",
        "drupal/mailchimp": "^1.11",
        "drupal/media_entity_generic": "^1.1",
        "drupal/menu_link_attributes": "1.x-dev",
        "drupal/menu_trail_by_path": "^1.1",
        "drupal/metatag": "^1.4",
        "drupal/migrate_tools": "^5.0",
        "drupal/oembed_providers": "^2.0",
        "drupal/paragraphs": "^1.9",
        "drupal/paragraphs_features": "^1.6",
        "drupal/paragraphs_frontend_ui": "^2.0@alpha",
        "drupal/pathauto": "^1.0",
        "drupal/pwa": "^1.5",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.0",
        "drupal/search_api": "^1.8",
        "drupal/search_api_autocomplete": "^1.4",
        "drupal/search_api_solr": "^4.0",
        "drupal/similarterms": "^1.5",
        "drupal/simple_sitemap": "^4.0",
        "drupal/styleguide": "2.x-dev",
        "drupal/subpathauto": "^1.0@beta",
        "drupal/tablefield": "2.x-dev",
        "drupal/taxonomy_menu_ui": "^2.0",
        "drupal/token": "^1.1",
        "drupal/views_block_filter_block": "^1.0@beta",
        "drupal/views_flipped_table": "^1.4",
        "drupal/viewsreference": "^2.0@alpha",
        "drupal/webform": "^6.2@dev",
        "drush/drush": "~10.0",
        "harvesthq/chosen": "1.8.7",
        "laminas/laminas-zendframework-bridge": "^1.4",
        "oomphinc/composer-installers-extender": "^2.0",
        "simple-icons/simple-icons": "4.2.0",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "behat/behat": "3.*",
        "behat/mink": "^1.7",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "1.3.x-dev",
        "dmore/behat-chrome-extension": "^1.3",
        "drupal/coder": "^8.3.1",
        "drush-ops/behat-drush-endpoint": "^9.3",
        "instaclick/php-webdriver": "^1.4",
        "jcalderonzumba/gastonjs": "^1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
        "justinrainbow/json-schema": "^5.2",
        "mikey179/vfsstream": "^1.2",
        "phpspec/prophecy": "^1.12",
        "phpunit/phpunit": "^9.5.11"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [],
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/core": {
                "MenuLinkContentAccessControlHandler does not allow view access without admin permission, making these entities inaccessible via REST, JSON API and GraphQL https://www.drupal.org/project/drupal/issues/2915792": "https://www.drupal.org/files/issues/2019-07-19/2915792-40.patch"
            },
            "drupal/bg_image_formatter": {
                "Using the same bg image on a page causes hash collision https://www.drupal.org/project/bg_image_formatter/issues/3086642": "https://www.drupal.org/files/issues/2019-10-08/hash-collision-3086642-2.patch"
            }
        }
    },
    "replace": {
        "drupal/drupal": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        },
        "bin-dir": "vendor/bin/",
        "vendor-dir": "vendor",
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true
        }
    }
}
