{
    "name": "solve-it-once/frost",
    "description": "Composer-based atomic template for Drupal",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Brad Czerniak",
            "homepage": "https://www.solveitonce.com",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "acquia/blt": "^13.7",
        "acquia/blt-behat": "^1.2",
        "acquia/blt-drupal-test": "^1.0",
        "composer/installers": "^1.12",
        "cweagans/composer-patches": "~1.0",
        "drupal/address": "~1.0",
        "drupal/admin_toolbar": "^3.1",
        "drupal/advagg": "^5.0",
        "drupal/antibot": "^2.0",
        "drupal/auditfiles": "^3.0@beta",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/bg_image_formatter": "^1.1",
        "drupal/block_class": "^2.0",
        "drupal/block_field": "^1.0@alpha",
        "drupal/captcha": "^1.0@beta",
        "drupal/chosen": "^3.0",
        "drupal/commerce": "^2",
        "drupal/commerce_add_to_cart_link": "^2.0",
        "drupal/commerce_paypal": "^1.0@RC",
        "drupal/commerce_recurring": "1.x-dev",
        "drupal/commerce_shipping": "^2.0@RC",
        "drupal/config_filter": "^2.2",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/config_split": "^2.0@beta",
        "drupal/core": "^9.2",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/ctools": "^4.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/diff": "^1.0@RC",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity": "~1.0",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_reference_display": "1.x-dev",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/facets": "^2.0",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "3.x-dev",
        "drupal/form_mode_manager": "^2.0@dev",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.0",
        "drupal/human_decimal": "^3.0",
        "drupal/iframe": "^2.6",
        "drupal/imageapi_optimize": "^4.0@beta",
        "drupal/imageapi_optimize_binaries": "^1.0@alpha",
        "drupal/imageapi_optimize_resmushit": "^2.0@beta",
        "drupal/imagemagick": "^3.3",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/jquery_ui_tabs": "^2.0",
        "drupal/jquery_ui_tooltip": "^2.0",
        "drupal/linked_field": "^1.1",
        "drupal/linkit": "5.x-dev",
        "drupal/mailchimp": "^2.2",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/media_library_edit": "^3.0",
        "drupal/media_entity_generic": "^1.1",
        "drupal/menu_link_attributes": "1.x-dev",
        "drupal/menu_trail_by_path": "^2.0",
        "drupal/metatag": "^1.4",
        "drupal/migrate_tools": "^6.0",
        "drupal/oembed_providers": "^2.0",
        "drupal/paragraphs": "^1.9",
        "drupal/paragraphs_features": "^1.6",
        "drupal/paragraphs_frontend_ui": "^2.0@alpha",
        "drupal/pathauto": "^1.0",
        "drupal/pwa": "^1.5",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.0",
        "drupal/search_api": "^1.8",
        "drupal/search_api_attachments": "^9.0",
        "drupal/search_api_autocomplete": "^1.4",
        "drupal/search_api_solr": "^4.0",
        "drupal/similarterms": "^1.5",
        "drupal/simple_sitemap": "^4.0",
        "drupal/styleguide": "2.x-dev",
        "drupal/subpathauto": "^1.0@beta",
        "drupal/tablefield": "2.x-dev",
        "drupal/taxonomy_menu_ui": "^3.0",
        "drupal/token": "^1.1",
        "drupal/views_block_filter_block": "^2.0",
        "drupal/views_flipped_table": "^1.4",
        "drupal/viewsreference": "^2.0@alpha",
        "drupal/webform": "^6.2@dev",
        "drush/drush": "^11.0",
        "harvesthq/chosen": "1.8.7",
        "laminas/laminas-zendframework-bridge": "^1.4",
        "mikemadison13/blt-lando": "dev-main",
        "oomphinc/composer-installers-extender": "^2.0",
        "simple-icons/simple-icons": "4.2.0",
        "tedivm/jshrink": "^1.4",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "acquia/blt-phpcs": "^1.0",
        "acquia/coding-standards": "^0.8.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^9",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "weitzman/drupal-test-traits": "^1.5"
    },
    "config": {
        "allow-plugins": {
            "acquia/blt": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true,
            "acquia/blt-phpcs": true
        },
        "bin-dir": "vendor/bin/",
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "gitignore": true,
            "locations": {
                "web-root": "./docroot"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset",
                "harvesthq/chosen"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "merge-plugin": {
            "include": [
                "docroot/modules/contrib/dropzonejs/composer.libraries.json",
                "docroot/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "MenuLinkContentAccessControlHandler does not allow view access without admin permission, making these entities inaccessible via REST, JSON API and GraphQL https://www.drupal.org/project/drupal/issues/2915792": "https://www.drupal.org/files/issues/2019-07-19/2915792-40.patch"
            },
            "drupal/bg_image_formatter": {
                "Using the same bg image on a page causes hash collision https://www.drupal.org/project/bg_image_formatter/issues/3086642": "https://www.drupal.org/files/issues/2019-10-08/hash-collision-3086642-2.patch"
            }
        },
        "violinist": {
            "allow_updates_beyond_constraint": 0,
            "blocklist": [
                "drupal/core-composer-scaffold"
            ],
            "bundled_packages": {
                "drupal/core-recommended": [
                    "drupal/core-composer-scaffold"
                ]
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "harvesthq/chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "chosen"
                },
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "simple-icons": {
            "type": "package",
            "package": {
                "name": "simple-icons/simple-icons",
                "version": "4.2.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "simple-icons"
                },
                "dist": {
                    "url": "https://github.com/simple-icons/simple-icons/archive/4.2.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    }
}
